<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/skin_background"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <com.tcn.vendingappplugin.MyTextView
        android:id="@+id/myTextView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:onClick="querySlot"
        android:padding="16dp"
        android:text="@string/skin_test_name"
        android:textColor="@color/skin_red"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.tcn.vendingappplugin.MyTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:onClick="querySlot"
        android:padding="16dp"
        android:text="@string/app_name"
        android:textColor="@color/skin_red"
        android:textSize="@dimen/skin_test11"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:onClick="startPlug"
        android:padding="16dp"
        android:text="test"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.tcn.romate.skin.TcnSkinEditText
        android:text="@string/skin_test_name"
        android:textColor="@color/skin_red"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </com.tcn.romate.skin.TcnSkinEditText>

    <com.tcn.romate.skin.TcnSkinButton
        android:text="@string/skin_test_name"
        android:textColor="@color/skin_red"
        android:layout_width="match_parent"
        android:textSize="@dimen/skin_test11"

        android:layout_height="wrap_content">

    </com.tcn.romate.skin.TcnSkinButton>

</LinearLayout>